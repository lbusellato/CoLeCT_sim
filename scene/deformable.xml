 <mujoco model="deformable">
   <worldbody>
   <flexcomp type="grid" count="5 5 5" spacing=".02 .02 .02" pos="0.5 0.5 0.04"
             radius=".0" rgba="0 .7 .7 1" name="softbody" dim="3" mass="1">
      <contact condim="3" solref="0.01 0.5" solimp=".95 .99 .0001" selfcollide="none"/>
      <edge damping="1"/>
      <plugin plugin="mujoco.elasticity.solid">
         <config key="poisson" value="0.1"/>
         <!--Units are in Pa (SI)-->
         <config key="young" value="1e5"/>
      </plugin>
   </flexcomp>
</worldbody>
</mujoco>